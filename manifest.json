{
  "name": "Hide Message fb",
  "version": "1.0",
  "description": "hidang",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
  {
    "matches": ["https://*.facebook.com/*"],
    "js": ["./controllers/show_hide.js", "./controllers/addEventMouse.js", "./controllers/checkChatTabInterval.js" ]
  }
  ],
  "permissions": ["tabs", "activeTab","declarativeContent", "storage", "https://*.facebook.com/*"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "manifest_version": 2
}